            "type": "string"
          },
          "name": "from",
          "in": "path",
          "required": true
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "until",
          "in": "path",
          "required": true
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "server_id",
          "in": "path",
          "allowEmptyValue": true
        }
